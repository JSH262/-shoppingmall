<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>

<div class="modal fade" id="customAlert" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="customAlertLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content rounded-3 shadow">
			<div class="modal-body p-4 text-center">
				<h5 class="mb-0" id="customAlertTitle">Enable this setting?</h5>
				<p class="mb-0" id="customAlertBody">You can always change your mind in your account settings.</p>
			</div>
			<div class="modal-footer flex-nowrap p-0" id="customAlertBtnGup">
				<button type="button" id="customAlertBtnOk" class="btn btn-lg btn-link fs-6 text-decoration-none col-6 m-0 rounded-0 border-end">
					<strong>확인</strong>
				</button>
				<button type="button" id="customAlertBtnCancel" class="btn btn-lg btn-link fs-6 text-decoration-none col-6 m-0 rounded-0">취소</button>
			</div>			
		</div>
	</div>
</div>